{
  "name": "x-pilot-remotion-template",
  "version": "1.0.0",
  "description": "Remotion template for AI-generated educational videos",
  "scripts": {
    "dev": "node scripts/scene-preflight.mjs && remotion studio",
    "build": "node scripts/scene-preflight.mjs && remotion bundle",
    "render": "node render.js",
    "render:cli": "node scripts/scene-preflight.mjs && remotion render MainVideo output.mp4",
    "refresh:scenes": "node scripts/scene-preflight.mjs",

    "bundle:api": "node scripts/bundle-worker.mjs",
    "render:api": "node scripts/render-worker.mjs",

    "upgrade": "remotion upgrade",
    "predev": "node scripts/scene-preflight.mjs",
    "prebuild": "node scripts/scene-preflight.mjs",
    "prerender:cli": "node scripts/scene-preflight.mjs",
    "lint": "eslint src --ignore-pattern \"src/scenes/**\" --ignore-pattern \"src/scene-registry.generated.tsx\" && tsc",
    "lint:scenes": "tsc -p tsconfig.scenes.json --noEmit && eslint src/scenes",

    "test:push": "bash test_push.sh",
    "e2b:build:dev": "cd x-pilot-remotion-template && npx tsx build.dev.ts",
    "e2b:build:prod": "cd x-pilot-remotion-template && npx tsx build.prod.ts"
  },
